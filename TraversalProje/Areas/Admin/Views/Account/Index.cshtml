@model AccountViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Unit Of Work Personel Prim Transferi</h1>

<form method="post">
    <label>Gönderen ID giriniz</label>
    <input type="text" class="form-control" asp-for="SenderID"/>
    <br />
    <label>Alıcı ID giriniz</label>
    <input type="text" class="form-control" asp-for="RecieverID" />
    <br />
    <label>Miktar Giriniz</label>
    <input type="text" class="form-control" asp-for="Amount" />
    <br />
    <button class="btn btn-outline-warning">Hesaba Aktar</button>
</form>